<%@ page pageEncoding="UTF-8"%>
<style>
.view_cll {
	display: none;
}
</style>
<div class="easyui-layout" data-options="fit:true">
	<div data-options="region:'south',border:false" style="height:40px;background:#eee;border-top: solid 1px #ccc;overflow: hidden;text-align: right;padding: 6px;">
		<a id="btnzz" href="#" class="easyui-linkbutton"><i class="fa fa-clipboard"></i>&nbsp;转账</a>
		<a id="btnzzClose" href="#" class="easyui-linkbutton"><i class="fa fa-times"></i>&nbsp;关闭</a>
	</div>
	<div data-options="region:'center',border:false" style="padding:9px;overflow: hidden;">
		<form id="zhuanzhangForm">
		<table>
			<tr>
				<th align="left">转出账户</th>
				<td>
					<input id="zhaunchuZhanghu" name="zhaunchuZhanghu" class="easyui-validatebox">
					<input type="hidden"  id="accountCurrency" name="accountCurrency" class="easyui-validatebox">
					<input type="hidden" id="zhuanchuKaihuhang" name="zhuanchuKaihuhang" class="easyui-validatebox">
					<input type="hidden"  id="zhuanruKaihuhang" name="zhuanruKaihuhang" class="easyui-validatebox"></br>
					<input type="hidden"  id="currencyType" name="currencyType" class="easyui-validatebox">
				</td>
			</tr>
			<tr>
				<th align="left">账户余额</th>
				<td >
					<input id="moneyYue" name="moneyYue" class="easyui-numberbox" data-options="required:true,precision:2">
				</td>
			</tr>
			<tr>
				<th align="left">转入账户</th>
				<td >
					<input id="zhuanruAccount" name="zhuanruAccount" class="easyui-combogrid" style="width:170px;">
				</td>
			</tr>
			<tr>
				<th align="left">转出金额</th>
				<td>
					<input  id="zhuchuMoney" name="zhuchuMoney" class="easyui-numberbox" data-options="required:true,precision:2">
				</td>
			</tr>
			<tr>
				<th align="left">手续费</th>
				<td>
					<input  id="factorage" name="factorage" class="easyui-numberbox" data-options="required:true,precision:2">
				</td>
			</tr>
			<tr>
				<th align="left">凭证号</th>
				<td>
					<input  id="voucherNo" name="voucherNo" class="easyui-validatebox" data-options="required:true">
				</td>
			</tr>
			<tr>
				<th align="left">交易日期</th>
				<td>
					<input  id="tradeDate" name="tradeDate" class="easyui-datebox" data-options="required:true,editable:false">
				</td>
			</tr>
		</table>
		</form>
	</div>
</div>

